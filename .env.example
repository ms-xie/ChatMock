# Port
PORT=8000

# Auth dir
CHATGPT_LOCAL_HOME=/data

# show request/stream logs
VERBOSE=true

# OAuth client id (modify only if you know what you're doing)
# CHATGPT_LOCAL_CLIENT_ID=app_EMoamEEZ73f0CkXaXp7hrann

# Reasoning controls
CHATGPT_LOCAL_REASONING_EFFORT=minimal       # minimal|low|medium|high
CHATGPT_LOCAL_REASONING_SUMMARY=auto        # auto|concise|detailed|none
CHATGPT_LOCAL_REASONING_COMPAT=think-tags   # legacy|o3|think-tags|current
CHATGPT_ENABLE_WEB_SEARCH=true

# API key your clients must present to this proxy; use your real OpenAI key if you plan to call OpenAI embeddings.
OPENAI_API_KEY=sk-...

# Optional suffix that must match what clients send; helps avoid accidentally hitting OpenAI directly.
API_KEY_CUSTOM_SUFFIX=-chatmock

# Point this at a self-hosted embeddings service; leave commented to call OpenAI's default endpoint instead.
# OPENAI_EMBEDDINGS_ENDPOINT=http://192.168.16.251:8088/v1/embeddings

# Force a specific model name
# CHATGPT_LOCAL_DEBUG_MODEL=gpt-5

# Preferred timezone for displaying account usage timestamps (e.g. 'America/New_York' or 'UTC+02')
# Leave unset to follow the system local timezone.
# CHATMOCK_USER_TZ=America/New_York
